<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <changeSet id="create_table_phones" author="robinx51">
        <createTable tableName="phones">
            <column name="model" type="varchar(255)">
                <constraints primaryKey="true" primaryKeyName="pk_client" nullable="false"/>
            </column>

            <column name="release_year" type="integer"/>

            <column name="type" type="jsonb"/>

            <column name="memory" type="jsonb"/>

            <column name="color" type="jsonb"/>
        </createTable>
    </changeSet>
</databaseChangeLog>